{
  "name": "john-deere-quick-sale",
  "version": "0.0.1",
  "private": true,
  "package": {
    "hasInstallScript": true
  },
  "scripts": {
    "update:pods": "pod install --project-directory=ios --repo-update",
    "apply-minor-updates": "npm install `npm outdated --json | node utilities/process-npm-outdated.js` --save-exact; npm dedupe",
    "create:toggle": "node utilities/add-toggle.js",
    "update:toggles": "utilities/apply_local_toggles.sh",
    "update:french": "node utilities/french-missing-keys.js",
    "android": "test -f .env && react-native run-android --active-arch-only || echo '.env is missing'",
    "android:default": "emulator -avd default -netdelay none -netspeed full & npm run android",
    "android:profile": "cd android && ./gradlew buildRelease --profile && cd ..",
    "bundle:android": "mkdir -p android/app/src/main/assets && react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
    "bundle:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "cd android && rm -rf ./app/src/main/res/drawable-* && rm -rf ./app/src/main/res/raw && ./gradlew assembleRelease",
    "doctor": "react-native doctor",
    "ios": "test -f .env && react-native run-ios || echo '.env is missing'",
    "ipad": "test -f .env && react-native run-ios --simulator='iPad Pro (9.7-inch)' || echo '.env is missing'",
    "start": "react-native start",
    "process-third-party-disclaimer": "node utilities/third-party-software-disclaimer.js",
    "test": "jest --coverage __tests__/unit",
    "test:ci": "jest --coverage --detectOpenHandles --forceExit __tests__/unit",
    "lint:js": "eslint src __tests__ testing utilities",
    "lint:json": "node utilities/lint-json-files.js",
    "lint:html": "eslint online-content/**/*.html && prettier --check online-content/**/*.html",
    "lint": "npm run lint:js && npm run lint:json npm run lint:html",
    "verify": "npm run lint && npm run test",
    "postinstall": "utilities/patch.sh"
  },
  "dependencies": {
    "@appdynamics/react-native-agent": "23.5.0",
    "@fullstory/react-native": "1.3.0",
    "@material-design-icons/svg": "0.14.13",
    "@okta/okta-react-native": "2.9.1",
    "@react-native-async-storage/async-storage": "1.19.3",
    "@react-native-community/clipboard": "1.5.1",
    "@react-native-community/netinfo": "9.4.1",
    "@react-native-picker/picker": "2.5.1",
