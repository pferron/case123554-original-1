    "json-diff": "1.0.6",
    "lodash.debounce": "4.0.8",
    "obfuscator-io-metro-plugin": "2.1.3",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-intl": "6.4.7",
